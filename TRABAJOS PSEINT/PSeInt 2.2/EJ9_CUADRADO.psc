
Algoritmo EJ9_CUADRADO
	
	Definir numlados,a,b Como Entero;
	
	Escribir "Ingrese la longitud de los lados:";
	Leer numlados;
	
	Para a = 1 Hasta numlados Con Paso 1 Hacer
		Para b = 1 Hasta numlados Con Paso 1 Hacer
			Si a = 1 O a = numlados O b = 1 O b = numlados Entonces
				Escribir Sin Saltar "* ";
			SiNo
				Escribir Sin Saltar "  ";
			FinSi
		Fin Para
		
		Escribir "";
	Fin Para


FinAlgoritmo
